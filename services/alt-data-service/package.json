{
  "name": "@ml-trading/alt-data-service",
  "version": "1.0.0",
  "description": "Alternative data service for news, social sentiment, and external data feeds",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "docker:build": "docker build -t alt-data-service .",
    "docker:run": "docker run -p 3007:3007 alt-data-service",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run"
  },
  "dependencies": {
    "@ml-trading/domain": "workspace:*",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "redis": "^4.6.12",
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
    "knex": "^3.1.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "sentiment": "^5.0.2",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "p-queue": "^8.0.1",
    "cron": "^3.1.6",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/cron": "^2.4.0",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.5",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "alternative-data",
    "news",
    "sentiment",
    "external-feeds",
    "fintech",
    "enterprise"
  ],
  "author": "ML Trading Platform Team",
  "license": "MIT"
}