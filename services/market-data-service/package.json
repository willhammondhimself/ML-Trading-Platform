{
  "name": "@ml-trading/market-data-service",
  "version": "1.0.0",
  "description": "Real-time market data service with WebSocket feeds and historical data management",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "docker:build": "docker build -t market-data-service .",
    "docker:run": "docker run -p 3002:3002 market-data-service"
  },
  "dependencies": {
    "@ml-trading/domain": "workspace:*",
    "socket.io": "^4.7.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "axios": "^1.6.0",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "@influxdata/influxdb-client": "^1.33.2",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "decimal.js": "^10.4.3",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.5",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "socket.io-client": "^4.7.5"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "market-data",
    "websocket",
    "real-time",
    "trading",
    "fintech",
    "enterprise"
  ],
  "author": "ML Trading Platform Team",
  "license": "MIT"
}